---
layout: default
---
  <div class="post">
    <header class="post-header">
      <h1 class="post-title">
        {% if site.title == 'blank' %}
          <strong><span style="
          font-size: 38px;
          letter-spacing: -1px;
          line-height: 0.6;
          margin-bottom: 10px;
          margin-top: 0;
          color: #666;">
          {{ site.first_name }}
        </span></strong>
          <span style="
            font-size: 38px;
            letter-spacing: -1px;
            line-height: 0.6;
            margin-bottom: 10px;
            margin-top: 0;
            color: #666;">
            {{ site.last_name }}
          </span>
          
          <strong></strong><span style="
              font-size: 38px;
              letter-spacing: -1px;
              line-height: 0.6;
              margin-bottom: 10px;
              margin-top: 0;
              color: #666;">
            ({{ site.Chinese_name }})
          </span></strong>
        {% else %}
          {{ site.title }}
        {% endif %}
      </h1>
      <p class="desc" style="font-size: 14px; color: #999;">
      <a style="color: #999; text-decoration: none;">
        M.Phil in Computer Sciences
      </a> • 
      <a style="color: #999; text-decoration: none;">
        Department of Computer Science and Engineering
      </a> • 
      <a href="https://www.seu.edu.cn" target="_blank" style="color: #999; text-decoration: none;">
        Southeast University
      </a>
    </p>
      </header>
  </div>

  <article>
    {% if page.profile %}
      <div class="profile float-{% if page.profile.align == 'left' %}left{% else %}right{% endif %}">
        {% if page.profile.image %}
          {% assign profile_image_path = page.profile.image | prepend: 'assets/img/' %}
          {% if page.profile.image_circular %}
            {% assign profile_image_class = 'img-fluid z-depth-1 rounded-circle' %}
          {% else %}
            {% assign profile_image_class = 'img-fluid z-depth-1
      rounded' %}
          {% endif %}
          {% capture sizes %}(min-width: {{ site.max_width }}) {{ site.max_width | minus: 30 | times: 0.3}}px, (min-width: 576px)
      30vw, 95vw"{% endcapture %}
          {%
            include figure.liquid loading="eager" path=profile_image_path class=profile_image_class sizes=sizes alt=page.profile.image
            cache_bust=true
          %}
        {% endif %}
        {% if page.profile.more_info %}
          <div class="more-info">{{ page.profile.more_info }}</div>
        {% endif %}
      </div>
    {% endif %}

    <div class="clearfix">{{ content }}</div>

    <!-- News -->
    {% if page.news and site.announcements.enabled %}
      <h2>
        <a style="color: inherit">👀 News</a>
      </h2>
      {% include news.liquid %}
    {% endif %}

  <!-- Education Experience -->
  {% if site.latest_posts.enabled %}
    <h2>
      <a style="color: inherit">👨🏻‍🎓 Education Experience</a>
    </h2>
    <ul style="list-style-type: none; padding: 0;">
      {% for education in site.education %}
        <li style="margin-bottom: 0.8em;">
          <strong>{{ education.school }}</strong> 
          <span style="color: #555;">| {{ education.degree }}</span><br>
          <span style="font-size: 0.9em; color: #777;">{{ education.year }}</span>
        </li>
      {% endfor %}
    </ul>
  {% else %}
    <p>Coming soon ... 🏗️ 🚧 🔨</p>
  {% endif %}

    <!-- Latest posts -->
    {% if site.latest_posts.enabled %}
      <h2>
        <a style="color: inherit">🌟 Selected Honors</a>
      </h2>
      Coming soon ... 🏗️ 🚧 🔨
    {% endif %}

    <!-- Selected papers -->
    {% if page.selected_papers %}
      <h2>
        <a href="{{ '/publications/' | relative_url }}" style="color: inherit">📖 Selected Publications</a>
      </h2>
      {% include selected_papers.liquid %}
    {% endif %}

    <h2>
      <a style="color: inherit">💡 Teaching Assistant</a>
    </h2>
    <a>2022 Fall </a>
    <div class="course-list">
            <ul>
              <li>Fundamental of Programming (for 2022 Mathematics and Computer Science Special Class, and WeBank Fintech Class)</li>
              <li>Data Structure (for 2021 Computer Science Class)</li>
            </ul>
        </div>
      <a>2023 Spring </a>
      <div class="course-list">
              <ul>
                <li>Object-Oriented Programming (for 2022 Mathematics and Computer Science Special Class, and WeBank Fintech Class)</li>
                <li>Computer System (for 2022 Mathematics and Computer Special Class)</li>
              </ul>
      </div>
      <a>2023 Fall </a>
      <div class="course-list">
              <ul>
                <li>Data Structure (for 2022 Computer Science Class)</li>
                <li>Intorduction of Computer (common optional course)</li>
              </ul>
      </div>
      <a> 2024 Spring </a>
      <div class="course-list">
              <ul>
                <li>Operating System (for 2021 Computer Science Class)</li>
                <li>Object-Oriented Programming (for 2023 Computer Science Class)</li>
              </ul>
      </div>

    <!-- Social -->
    {% if page.social %}
      <div class="social">
        <div class="contact-icons">{% include social.liquid %}</div>

        <div class="contact-note">{{ site.contact_note }}</div>
      </div>
    {% endif %}




    {% if site.newsletter.enabled and site.footer_fixed %}
      {% include scripts/newsletter.liquid center=true %}
    {% endif %}
  </article>
</div>
